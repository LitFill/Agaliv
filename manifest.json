{
	"manifest_version": 3,
	"name": "Agaliv",
	"version": "1.0.0",
	"description": "Auto Close Tabs Extension",
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/icon16.png",
			"48": "images/icon48.png",
			"128": "images/icon128.png"
		}
	},
	"background": {
		"service_worker": "service_worker.js"
	}
}
